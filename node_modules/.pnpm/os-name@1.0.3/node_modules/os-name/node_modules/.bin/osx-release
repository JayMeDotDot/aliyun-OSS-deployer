#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zorba/Desktop/myRep/aliyun-OSS-deployer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zorba/Desktop/myRep/aliyun-OSS-deployer/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../osx-release@1.1.0/node_modules/osx-release/cli.js" "$@"
else
  exec node  "$basedir/../../../../../osx-release@1.1.0/node_modules/osx-release/cli.js" "$@"
fi
